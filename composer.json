{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "archtechx/laravel-seo": "^0.2.2",
    "blade-ui-kit/blade-heroicons": "^1.2",
    "blade-ui-kit/blade-ui-kit": "^0.2.0",
    "cyrildewit/eloquent-viewable": "^6.0",
    "doctrine/dbal": "^3.1",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "graham-campbell/markdown": "^13.1",
    "guzzlehttp/guzzle": "^7.0.1",
    "jenssegers/agent": "^2.6",
    "laravel-notification-channels/telegram": "^0.7.0",
    "laravel-notification-channels/twitter": "^5.1",
    "laravel/fortify": "^1.7",
    "laravel/framework": "^v8.70",
    "laravel/slack-notification-channel": "^2.3",
    "laravel/socialite": "^5.2",
    "laravel/tinker": "^2.5",
    "livewire/livewire": "^2.5",
    "ph7jack/wireui": "^0.14.0",
    "ramsey/uuid": "^4.2",
    "sentry/sentry-laravel": "^2.10",
    "spatie/laravel-feed": "^4.0",
    "spatie/laravel-medialibrary-pro": "^1.0.0",
    "spatie/laravel-permission": "^5.1",
    "spatie/laravel-sitemap": "^6.0",
    "stevebauman/location": "^6.2",
    "torchlight/torchlight-commonmark": "^0.5.2",
    "wire-elements/modal": "^1.0",
    "wire-elements/spotlight": "^1.0",
    "yarri/link-finder": "^2.7"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.6",
    "brianium/paratest": "^6.3",
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "friendsofphp/php-cs-fixer": "^3.0",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "pestphp/pest-plugin-laravel": "^1.1",
    "pestphp/pest-plugin-livewire": "^1.0",
    "phpunit/phpunit": "^9.3.3",
    "spatie/test-time": "^1.2"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "sniff": [
      "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
    ],
    "lint": [
      "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
    ],
    "pest": [
      "./vendor/bin/pest"
    ],
    "setup": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "php -r \"file_exists('auth.json') || copy('auth.json.example', 'auth.json');\"",
      "php artisan key:generate",
      "php artisan migrate --seed",
      "php artisan lcm:admin"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://satis.spatie.be"
    }
  ]
}
